{
"manifest_version": 2,
    "name": "slanguage",
    "description": "slanguage man",
    "version": "1.0",
    "permissions": [
        "storage"
      ],
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "browser_action": {
        "default_popup": "index.html",
        "default_title": "Open the popup"
      },
      "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
      "icons": {
        "16": "logo-small.png",
        "48": "logo-small.png",
        "128": "logo-small.png"
      }
}